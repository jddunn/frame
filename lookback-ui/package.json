    "scripts": {
        "start": "webpack-dev-server --open --config webpack.dev.js --progress --color",
        "build": "webpack --config webpack.prod.js",
        "build-dev": "webpack --config webpack.dev.js --progress --color",
        "lint": "eslint -c .eslintrc.json src/** src/**/**; exit 0",
        "flow": "flow",
        "flow start": "flow start",
        "flow stop": "flow stop",
        "flow status": "flow status",
        "flow coverage": "flow coverage",
        "electron-prod": "webpack --config webpack.prod.electron.js && electron --noDevServer .",
        "electron-dev": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.electron.js",
        "electron-build": "./node_modules/.bin/electron-builder build"
    },
